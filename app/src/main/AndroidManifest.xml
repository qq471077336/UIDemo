<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lwd.uidemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UIDemo">
        <activity android:name=".text.ColorChangeTextViewActivity"></activity>
        <activity android:name=".cardslide.CardSlideActivity" />
        <activity android:name=".stickyheader.StickyHeaderActivity" />
        <activity android:name=".fish.FishActivity" />
        <activity android:name=".nested.NestedActivity" />
        <activity android:name=".skin.SkinActivity" />
        <activity android:name=".flow.FlowActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>